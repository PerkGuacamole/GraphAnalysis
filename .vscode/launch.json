{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "(gdb) Launch",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/graph_analysis",
			"args": [],
			"stopAtEntry": true,
			"cwd": "${workspaceFolder}",
			"environment": [
				{
					"name": "out_dir",
					"value": "./output/debug/"
				},
				{
					"name": "alloc_size",
					"value": "1000"
				},
				{
					"name": "num_vertices",
					"value": "1000"
				},
				{
					"name": "min_degree",
					"value": "1"
				},
				{
					"name": "max_degree",
					"value": "2"
				},
				{
					"name": "min_value",
					"value": "1"
				},
				{
					"name": "max_value",
					"value": "2"
				},
				{
					"name": "page_rank_iterations",
					"value": "1"
				},
				{
					"name": "page_rank_dampening_factor",
					"value": "0.5"
				},
				{
					"name": "test_iterations",
					"value": "1"
				}
			],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		}
	]
}